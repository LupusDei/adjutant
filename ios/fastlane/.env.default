# Fastlane Environment Variables Template
# Copy this to .env and fill in your actual values
# NEVER commit .env to git - it contains secrets!

# App Store Connect API Key (preferred auth method for CI)
# Generate at: https://appstoreconnect.apple.com/access/api
APP_STORE_CONNECT_API_KEY_ID=
APP_STORE_CONNECT_API_ISSUER_ID=
APP_STORE_CONNECT_API_KEY_PATH=./AuthKey.p8

# Alternative: Apple ID authentication (for local dev)
APPLE_ID=your-apple-id@example.com
FASTLANE_APPLE_APPLICATION_SPECIFIC_PASSWORD=

# App identifiers
APP_IDENTIFIER=com.jmm.Adjutant
TEAM_ID=2BRYAM5Q52
ITC_TEAM_ID=

# Match configuration (certificate management)
# Create a PRIVATE repo for storing encrypted certs
MATCH_GIT_URL=git@github.com:your-org/certificates.git
MATCH_PASSWORD=your-encryption-password

# Optional: Provisioning profile name (if not using match naming)
PROVISIONING_PROFILE_NAME=

# Build changelog for TestFlight
CHANGELOG=New build from development
